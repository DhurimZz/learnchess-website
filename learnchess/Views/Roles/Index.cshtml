@model List<Microsoft.AspNetCore.Identity.IdentityRole>
@{
    ViewData["Title"] = "Roles Menager";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h1>Role Menager</h1>
<table class="table">
    <thead>
        <tr>
           <th>Id</th>
           <th>Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var role in Model)
        {
            <tr>
            <td>@role.Id</td>
            <td>@role.Name</td>
            </tr>
        }        
    </tbody>
</table>